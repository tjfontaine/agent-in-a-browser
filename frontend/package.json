{
    "name": "web-agent-frontend",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "dev": "vite",
        "build": "npm run transpile:all && tsc && vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:docker": "docker compose -f docker-compose.e2e.yml up --build --abort-on-container-exit",
        "test:all": "npm run test && npm run test:e2e",
        "transpile:component:jspi": "jco transpile ../target/wasm32-wasip2/release/ts-runtime-mcp.wasm -o src/wasm/mcp-server-jspi --async-mode jspi --async-imports 'mcp:module-loader/loader#get-lazy-module' --async-imports 'mcp:module-loader/loader#spawn-lazy-command' --async-imports 'mcp:module-loader/loader#spawn-interactive' --async-imports 'mcp:module-loader/loader#[method]lazy-process.try-wait' --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'wasi:io/streams#[method]output-stream.blocking-write-and-flush' --async-imports 'wasi:filesystem/types#[method]descriptor.read-directory' --async-imports 'wasi:filesystem/types#[method]descriptor.open-at' --async-imports 'wasi:filesystem/types#[method]descriptor.stat' --async-imports 'wasi:filesystem/types#[method]descriptor.stat-at' --async-imports 'wasi:filesystem/types#[method]descriptor.create-directory-at' --async-imports 'wasi:filesystem/types#[method]descriptor.unlink-file-at' --async-imports 'wasi:filesystem/types#[method]descriptor.remove-directory-at' --async-imports 'wasi:filesystem/types#[method]descriptor.rename-at' --async-imports 'wasi:filesystem/types#[method]descriptor.symlink-at' --async-exports 'wasi:cli/run@0.2.6#run' --async-exports 'wasi:http/incoming-handler@0.2.4#handle' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../../../../packages/wasi-shims/src/clocks-impl.js#*' --map 'wasi:filesystem/*=../../../../packages/wasi-shims/src/opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../../../../packages/wasi-shims/src/wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../../../../packages/wasi-shims/src/wasi-http-impl.js#outgoingHandler' --map 'mcp:module-loader/loader=../lazy-loading/module-loader-impl.js' --valid-lifting-optimization --name ts-runtime-mcp",
        "transpile:component:sync": "jco transpile ../target/wasm32-wasip2/release/ts-runtime-mcp.wasm -o src/wasm/mcp-server-sync --async-mode sync --tla-compat --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../../../../packages/wasi-shims/src/clocks-impl.js#*' --map 'wasi:filesystem/*=../../../../packages/wasi-shims/src/opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../../../../packages/wasi-shims/src/wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../../../../packages/wasi-shims/src/wasi-http-impl.js#outgoingHandler' --map 'mcp:module-loader/loader=../lazy-loading/module-loader-impl.js' --valid-lifting-optimization --name ts-runtime-mcp",
        "transpile:component": "npm run transpile:component:jspi && npm run transpile:component:sync",
        "transpile:tsx-engine": "jco transpile ../target/wasm32-wasip2/release/tsx_engine.wasm -o ../packages/wasm-tsx/wasm --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'wasi:io/streams#[method]output-stream.blocking-write-and-flush' --async-imports 'wasi:filesystem/types#[method]descriptor.read-directory' --async-imports 'wasi:filesystem/types#[method]descriptor.open-at' --async-imports 'wasi:filesystem/types#[method]descriptor.stat' --async-imports 'wasi:filesystem/types#[method]descriptor.stat-at' --async-imports 'wasi:filesystem/types#[method]descriptor.create-directory-at' --async-imports 'wasi:filesystem/types#[method]descriptor.unlink-file-at' --async-imports 'wasi:filesystem/types#[method]descriptor.remove-directory-at' --async-imports 'wasi:filesystem/types#[method]descriptor.rename-at' --async-imports 'wasi:filesystem/types#[method]descriptor.symlink-at' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../../wasi-shims/src/clocks-impl.js#*' --map 'wasi:filesystem/*=../../wasi-shims/src/opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../../wasi-shims/src/wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../../wasi-shims/src/wasi-http-impl.js#outgoingHandler' --valid-lifting-optimization --name tsx-engine",
        "transpile:sqlite-module": "jco transpile ../target/wasm32-wasip2/release/sqlite_module.wasm -o ../packages/wasm-sqlite/wasm --tla-compat --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../../wasi-shims/src/clocks-impl.js#*' --map 'wasi:filesystem/*=../../wasi-shims/src/opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --valid-lifting-optimization --name sqlite-module",
        "transpile:ratatui-demo": "jco transpile ../target/wasm32-wasip2/release/ratatui_demo.wasm -o ../packages/wasm-ratatui/wasm --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../../wasi-shims/src/clocks-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --valid-lifting-optimization --name ratatui-demo",
        "transpile:edtui-module": "jco transpile ../target/wasm32-wasip2/release/edtui_module.wasm -o ../packages/wasm-vim/wasm --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'wasi:io/streams#[method]output-stream.blocking-write-and-flush' --async-imports 'wasi:filesystem/types#[method]descriptor.open-at' --async-imports 'wasi:filesystem/types#[method]descriptor.stat' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=../../wasi-shims/src/ghostty-cli-shim.js#*' --map 'wasi:clocks/*=../../wasi-shims/src/clocks-impl.js#*' --map 'wasi:filesystem/*=../../wasi-shims/src/opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'terminal:info/size=../../wasi-shims/src/ghostty-cli-shim.js#size' --valid-lifting-optimization --name edtui-module",
        "transpile:web-agent-tui": "jco transpile ../target/wasm32-wasip2/release/web_agent_tui.wasm -o src/wasm/web-agent-tui --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'shell:unix/command@0.1.0#run' --async-exports 'run' --map 'wasi:cli/*=../../../../packages/wasi-shims/src/ghostty-cli-shim.js#*' --map 'wasi:clocks/*=../../../../packages/wasi-shims/src/clocks-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:http/types=../../../../packages/wasi-shims/src/wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../../../../packages/wasi-shims/src/wasi-http-impl.js#outgoingHandler' --map 'shell:unix/command@0.1.0=../mcp-server-jspi/ts-runtime-mcp.js#command' --map 'terminal:info/size=../../../../packages/wasi-shims/src/ghostty-cli-shim.js#size' --valid-lifting-optimization --name web-agent-tui",
        "transpile:all": "npm run transpile:component && npm run transpile:tsx-engine && npm run transpile:sqlite-module && npm run transpile:ratatui-demo && npm run transpile:edtui-module && npm run transpile:web-agent-tui",
        "clean:wasm": "rm -rf src/wasm/mcp-server-jspi src/wasm/mcp-server-sync src/wasm/tsx-engine src/wasm/sqlite-module src/wasm/ratatui-demo src/wasm/edtui-module src/wasm/web-agent-tui"
    },
    "dependencies": {
        "ghostty-web": "^0.4.0",
        "isomorphic-git": "^1.36.1",
        "vite-plugin-node-polyfills": "^0.24.0"
    },
    "devDependencies": {
        "@bytecodealliance/jco": "^1.15.4",
        "@bytecodealliance/preview2-shim": "0.17.5",
        "@eslint/js": "^9.39.2",
        "@playwright/test": "^1.57.0",
        "@types/node": "^25.0.3",
        "@vitest/coverage-v8": "^4.0.16",
        "eslint": "^9.39.2",
        "jsdom": "^27.3.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.50.0",
        "vite": "^7.3.0",
        "vitest": "^4.0.16"
    }
}