{
    "name": "web-agent-frontend",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "dev": "vite",
        "build": "npm run transpile:all && tsc && vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:docker": "docker compose -f docker-compose.e2e.yml up --build --abort-on-container-exit",
        "test:all": "npm run test && npm run test:e2e",
        "transpile:component:jspi": "jco transpile ../target/wasm32-wasip2/release/ts-runtime-mcp.wasm -o src/wasm/mcp-server-jspi --async-mode jspi --async-imports 'mcp:module-loader/loader#get-lazy-module' --async-imports 'mcp:module-loader/loader#spawn-lazy-command' --async-imports 'mcp:module-loader/loader#[method]lazy-process.try-wait' --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'wasi:filesystem/types#[method]descriptor.read-directory' --async-imports 'wasi:filesystem/types#[method]descriptor.open-at' --async-imports 'wasi:filesystem/types#[method]descriptor.stat' --async-exports 'wasi:cli/run@0.2.6#run' --async-exports 'wasi:http/incoming-handler@0.2.4#handle' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:filesystem/*=../opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../wasi-http-impl.js#outgoingHandler' --map 'mcp:module-loader/loader=../module-loader-impl.js' --valid-lifting-optimization --name ts-runtime-mcp",
        "transpile:component:sync": "jco transpile ../target/wasm32-wasip2/release/ts-runtime-mcp.wasm -o src/wasm/mcp-server-sync --async-mode sync --tla-compat --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:filesystem/*=../opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../wasi-http-impl.js#outgoingHandler' --map 'mcp:module-loader/loader=../module-loader-impl.js' --valid-lifting-optimization --name ts-runtime-mcp",
        "transpile:component": "npm run transpile:component:jspi && npm run transpile:component:sync",
        "transpile:tsx-engine": "jco transpile ../target/wasm32-wasip2/release/tsx_engine.wasm -o src/wasm/tsx-engine --tla-compat --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:filesystem/*=../opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --map 'wasi:http/types=../wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../wasi-http-impl.js#outgoingHandler' --valid-lifting-optimization --name tsx-engine",
        "transpile:sqlite-module": "jco transpile ../target/wasm32-wasip2/release/sqlite_module.wasm -o src/wasm/sqlite-module --tla-compat --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:filesystem/*=../opfs-filesystem-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:sockets/*=@bytecodealliance/preview2-shim/sockets#*' --valid-lifting-optimization --name sqlite-module",
        "transpile:ratatui-demo": "jco transpile ../target/wasm32-wasip2/release/ratatui_demo.wasm -o src/wasm/ratatui-demo --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --valid-lifting-optimization --name ratatui-demo",
        "transpile:brush-shell": "jco transpile ../target/wasm32-wasip2/release/brush_shell.wasm -o src/wasm/brush-shell --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-exports 'shell:unix/command@0.1.0#run' --map 'wasi:cli/*=@bytecodealliance/preview2-shim/cli#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'mcp:module-loader/loader=../module-loader-impl.js' --valid-lifting-optimization --name brush-shell",
        "transpile:web-agent-tui": "jco transpile ../target/wasm32-wasip2/release/web_agent_tui.wasm -o src/wasm/web-agent-tui --async-mode jspi --async-imports 'wasi:io/streams#[method]input-stream.blocking-read' --async-imports 'shell:unix/command@0.1.0#run' --async-exports 'run' --map 'wasi:cli/*=../ghostty-cli-shim.js#*' --map 'wasi:clocks/*=../clocks-impl.js#*' --map 'wasi:io/*=@bytecodealliance/preview2-shim/io#*' --map 'wasi:random/*=@bytecodealliance/preview2-shim/random#*' --map 'wasi:http/types=../wasi-http-impl.js' --map 'wasi:http/outgoing-handler=../wasi-http-impl.js#outgoingHandler' --map 'shell:unix/command@0.1.0=../mcp-server-jspi/ts-runtime-mcp.js#command' --map 'terminal:info/size=../terminal-info-impl.js' --valid-lifting-optimization --name web-agent-tui",
        "transpile:all": "npm run transpile:component && npm run transpile:tsx-engine && npm run transpile:sqlite-module && npm run transpile:ratatui-demo && npm run transpile:brush-shell && npm run transpile:web-agent-tui",
        "clean:wasm": "rm -rf src/wasm/mcp-server-jspi src/wasm/mcp-server-sync src/wasm/tsx-engine src/wasm/sqlite-module src/wasm/ratatui-demo src/wasm/brush-shell src/wasm/web-agent-tui"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.56",
        "@ai-sdk/mcp": "^0.0.12",
        "@ai-sdk/openai": "^2.0.88",
        "@anthropic-ai/sdk": "^0.32.1",
        "@inkjs/ui": "^2.0.0",
        "@xterm/addon-fit": "^0.10.0",
        "@xterm/addon-search": "^0.15.0",
        "@xterm/addon-web-links": "^0.11.0",
        "@xterm/xterm": "^5.5.0",
        "ai": "^5.0.116",
        "buffer": "^6.0.3",
        "ghostty-web": "^0.4.0",
        "ink": "^6.5.1",
        "ink-web": "^0.1.11",
        "isomorphic-git": "^1.36.1",
        "react": "^19.2.3",
        "react-compiler-runtime": "^19.0.0-beta-af1b7da-20250417",
        "react-dom": "^19.2.3",
        "vite-plugin-node-polyfills": "^0.24.0",
        "xterm": "^5.3.0",
        "xterm-readline": "^1.1.2",
        "zod": "^4.2.1",
        "zustand": "^5.0.9"
    },
    "devDependencies": {
        "@bytecodealliance/jco": "^1.15.4",
        "@bytecodealliance/preview2-shim": "^0.17.5",
        "@eslint/js": "^9.39.2",
        "@playwright/test": "^1.57.0",
        "@types/node": "^22.10.2",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.16",
        "babel-plugin-react-compiler": "^1.0.0",
        "eslint": "^9.39.2",
        "eslint-plugin-react-compiler": "^19.1.0-rc.2",
        "jsdom": "^27.3.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.50.0",
        "vite": "^7.3.0",
        "vitest": "^4.0.16"
    }
}