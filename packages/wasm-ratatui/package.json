{
    "name": "@tjfontaine/wasm-ratatui",
    "version": "0.1.0",
    "type": "module",
    "main": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./wasm/*": "./wasm/*",
        "./wasm-sync/*": "./wasm-sync/*"
    },
    "scripts": {
        "transpile": "node ../../scripts/transpile.mjs ratatui-demo",
        "transpile:sync": "node ../../scripts/transpile.mjs ratatui-demo --sync",
        "clean": "rm -rf wasm wasm-sync"
    },
    "dependencies": {
        "@tjfontaine/wasm-loader": "workspace:*"
    }
}